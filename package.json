{
  "name": "node-js-express-rest-web-services-update",
  "version": "0.6.5",
  "description": "RESTful Web Services with Node.js and Express",
  "main": "app.js",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.7"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "nodemon app.js"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": 2500,
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/human-inhabitant/node-js-express-rest-web-services-update.git"
  },
  "keywords": [
    "Pluralsight",
    "Node.js",
    "Express"
  ],
  "author": "human-inhabitant",
  "license": "copyleft-next-0.3.1",
  "bugs": {
    "url": "https://github.com/human-inhabitant/node-js-express-rest-web-services-update/issues"
  },
  "homepage": "https://github.com/human-inhabitant/node-js-express-rest-web-services-update#readme"
}
